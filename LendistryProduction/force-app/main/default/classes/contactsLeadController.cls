    @AuraEnabled(cacheable = true)
    public static List<wrapper> getContactANDLead( String accId ){
        set<String> setOfConIds = new set<string>(); 
        list<wrapper> lstwrapper = new list<wrapper>();
        list<contact> lstContacts = [ Select Id, Name from Contact Where AccountId=:accId];
        for( contact eachCon : lstContacts )
        {
            setOfConIds.add(eachCon.Id);
        }
        List<Lead> lstOfLeads = [Select Id, Name , Status, Referral_Partner__c, Referral_Partner__r.Name , Referral_Partner__r.id, createdDate from Lead where IsConverted=false AND Referral_Partner__r.id IN:setOfConIds ];
        for(Lead each : lstOfLeads )
        {
            for( Contact eachCon : lstContacts )
            {
                
                wrapper objwrapper = new wrapper();
                if( eachCon.Id == each.Referral_Partner__r.id )
                {
                    objwrapper.LeadName = each.Name;
                    objwrapper.LeadId = each.Id;
                    objwrapper.LeadStatus = each.Status;
                    objwrapper.ContactId= each.Referral_Partner__r.id;    
                    objwrapper.ContactName = each.Referral_Partner__r.Name;
                    objwrapper.CreatedDate = string.valueof(each.createdDate );
                    lstwrapper.add(objwrapper); 
                }
            }
        }
        
        
        return lstwrapper;
    }
