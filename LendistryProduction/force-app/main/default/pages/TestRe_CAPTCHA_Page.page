function ChangeBusinessList() {
    var businessSel = document.getElementById("business_dotype"),
        businessTypeSel = document.getElementById("businesstypeLT"),
        businessTellmore = document.getElementById("morebusinessLT");
    for (var business in businessObject) {
        businessSel.options[businessSel.options.length] = new Option(business, business);
    }
    businessSel.onchange = function () {
        businessTypeSel.length = 1; 
        businessTellmore.length = 1; 
        if (this.selectedIndex < 1) return;    
        for (var businesstype in businessObject[this.value]) {
            businessTypeSel.options[businessTypeSel.options.length] = new Option(businesstype, businesstype);
        }
    }
    businessSel.onchange(); 
    businessTypeSel.onchange = function () {
        businessTellmore.length = 1; 
        if (this.selectedIndex < 1) return;    
        var businessmore = businessObject[businessSel.value][this.value];
        for (var i = 0; i < businessmore.length; i++) {
            businessTellmore.options[businessTellmore.options.length] = new Option(businessmore[i], businessmore[i]);
        }
    }
    
} 
        
