    <script>
    function ValidateEmail(mail) {
    
      commomValidate('email',mail);
    }
    function ValidatePhone(phone) {
    
      commomValidate('phone',phone);
    }
    function ValidateBusinessPhone(Business) {
    
      commomValidate('Business',Business);
    }
    function ValidateSSN(ssn) {
    
      commomValidate('ssn',ssn);
    }
    
    function ValidateRequestedAmount(Amount){
        commomValidate('RequestedAmount',Amount);
    }
    function ValidateWebsite(Website){
        commomValidate('WebSiteAddress',Website);
    }
    function ValidateRent(Rent){
        commomValidate('MonthlyRent',Rent);
    }
    
     function commomValidate(type,val){
         if(type=='MonthlyRent'){
             var num1 = val;
 if(isNaN(num1)){
        document.getElementById("errorRent").innerHTML = 'Enter a Valid Number';
 }
}
         
        if(type=='WebSiteAddress'){
             var Webformat = /^((ftp|http|https):\/\/)?(www.)?(?!.*(ftp|http|https|www.))[a-zA-Z0-9_-]+(\.[a-zA-Z]+)+((\/)[\w#]+)*(\/\w+\?[a-zA-Z0-9_]+=\w+(&[a-zA-Z0-9_]+=\w+)*)?$/;
             if(val.match(Webformat)){
                 document.getElementById("errorWebsite").innerHTML = '';
             }
             else{
                 document.getElementById("j_id0:mainFormId:LT_website").style.border="2px solid rgba(255, 0, 0, 0.7)";
                  document.getElementById("errorWebsite").innerHTML = 'Enter a Valid Webaddress';
             
             }
             
         }
         
         if(type=='email'){
             var mailformat = /^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$/;
             if(val.match(mailformat)){
                 document.getElementById("errorEmailId").innerHTML = '';
             }
             else{
                 document.getElementById("j_id0:mainFormId:email").style.border="2px solid rgba(255, 0, 0, 0.7)";
                  document.getElementById("errorEmailId").innerHTML = 'Invalid Email Format';
             
             }
             
         }
         
         if(type=='RequestedAmount'){
             //var mailformat = /^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$/;
             if(val>=10000 && val<=250000){
                 document.getElementById("errorRequestedAmount").innerHTML = '';
             }
             else{
                 document.getElementById("j_id0:mainFormId:RequestedAmount").style.border="2px solid rgba(255, 0, 0, 0.7)";
                  document.getElementById("errorRequestedAmount").innerHTML = 'Amount Requested should be between 10000 and 250000';
             
             }
             
         }
         
          if(type=='phone'){
            var phoneNumberPattern = /^(\()?\d{3}(\))?(-|\s)?\d{3}(-|\s)\d{4}$/;
             if(val.match(phoneNumberPattern )){
                 document.getElementById("errorPhoneId").innerHTML = '';
             }
             else{
              document.getElementById("j_id0:mainFormId:LT_Phone").style.border="2px solid rgba(255, 0, 0, 0.7)";   
              document.getElementById("errorPhoneId").innerHTML = 'Enter Phone in format XXX-XXX-XXXX';
                 }
             
         }
         if(type=='Business'){
            var phoneNumberPattern = /^(\()?\d{3}(\))?(-|\s)?\d{3}(-|\s)\d{4}$/;
             if(val.match(phoneNumberPattern )){
                 document.getElementById("errorBusinessPhoneId").innerHTML = '';
             }
             else{
                 document.getElementById("j_id0:mainFormId:Business").style.border="2px solid rgba(255, 0, 0, 0.7)";
              document.getElementById("errorBusinessPhoneId").innerHTML = 'Enter Phone in format XXX-XXX-XXXX';
             
         }
         }
         
         if(type=='ssn'){
            var ssnformat = /^(1\s?)?((\([0-9]{3}\))|[0-9]{3})[\s\-]?[\0-9]{3}[\s\-]?[0-9]{4}$/;
             if(val.match(ssnformat)){
                document.getElementById("errorSSNId").innerHTML = '';
             }
             else{
                 document.getElementById("j_id0:mainFormId:LT_SSN").style.border="2px solid rgba(255, 0, 0, 0.7)";
              document.getElementById("errorSSNId").innerHTML = 'Enter SSN in format XXX-XX-XXXX';
             }
         }
             
         }
         
</script>
    
